@using GameInterface.WebAPI.Models
<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta charset="utf-8" />

    <title>@ViewBag.Title</title>
    
    <link rel="stylesheet" href="@Url.Content("~/Content/Kendo/web/kendo.common.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/Kendo/web/kendo.default.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/Kendo/shared/examples-offline.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/Default.css")">
    <script src="@Url.Content("~/Scripts/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo.web.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo.aspnetmvc.min.js")"></script>
    <script src="@Url.Content("~/Scripts/console.min.js")"></script>
    <script src="@Url.Content("~/Scripts/prettify.min.js")"></script>

    @* Include the JavaScript for the current culture *@

    @{
        var culture = System.Threading.Thread.CurrentThread.CurrentCulture.ToString();
    }

    <script src="@Url.Content("~/Scripts/cultures/kendo.culture." + culture + ".min.js")"></script>

    @* Set the current culture *@

    <script>
        kendo.culture("@culture");
    </script>
   @* @RenderSection("HeadContent", false)*@
    @*@Html.Partial("_Header")*@

    <link href="@Url.Content("~/Content/themes/css/metro.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap-3.2.0-dist/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap-3.2.0-dist/css/bootstrap-theme.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Css/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/zTree/Css/zTreeStyle.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/themes/css/bootstrap-responsive.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/css/font-awesome.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/css/style.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/css/style_responsive.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/css/style_light.css")" rel="stylesheet" data-color="default" id="style_color" />
    <link href="@Url.Content("~/Content/themes/css/jquery.gritter.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/css/uniform.default.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/css/Layout.css")" rel="stylesheet" type="text/css" />


    <script src="@Url.Content("~/Content/themes/js/jquery-1.8.3.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap-3.2.0-dist/js/bootstrap.min.js")" type="text/javascript"></script>
    
    <script src="@Url.Content("~/Content/themes/js/breakpoints.js")"></script>
    <script src="@Url.Content("~/Content/themes/js/jquery.uniform.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/themes/js/jquery.gritter.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/themes/js/jquery.pulsate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/themes/js/app.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/themes/js/jquery.blockui.js")"></script>
    <script src="@Url.Content("~/Content/themes/js/jquery.cookie.js")"></script>

    <script src="@Url.Content("~/Content/zTree/js/jquery.ztree.core-3.5.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/zTree/js/jquery.ztree.excheck-3.5.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/zTree/js/jquery.ztree.exedit-3.5.js")" type="text/javascript"></script>

</head>
<body>
    <div class="page">
        <div>
            <div id="welcome" class="IndexTitle">欢迎您， @if (GlobalVariables.CurrentUser != null) { @GlobalVariables.CurrentUser.UserName; }  @Html.ActionLink("注销", "Logout", "Login")  @Html.ActionLink("主页", "Index", "ParticipateConsultation")</div>
        </div>
        <div id="example" class="k-content">
            @RenderBody()
        </div>
    </div>
    <div class="footer">
        @*@Html.Partial("_Footer")*@
    </div>
</body>
</html>
